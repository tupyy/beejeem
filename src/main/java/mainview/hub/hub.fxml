               <HBox.margin>
                  <Insets />
               </HBox.margin></TextField>
         </children>
      </HBox>
   </top>
   <center>
      <TableView fx:id="hubTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
