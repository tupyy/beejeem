<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="464.0" prefWidth="362.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mainview.sidepanel.modules.ModulesController">
   <children>
      <TableView fx:id="modulesTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn id="moduleNameColumn" fx:id="moduleNameColumn" maxWidth="400.0" minWidth="-1.0" prefWidth="-1.0" sortable="false" text="Name" />
            <TableColumn id="triggerColumn" fx:id="triggerColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" sortable="false" text="Trigger" />
         </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
         <children>
            <Button fx:id="addModuleButton" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Add" />
            <Separator prefWidth="10.0" visible="false" HBox.hgrow="NEVER" />
            <Button fx:id="removeButtonModule" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Remove" />
            <Separator layoutX="62.0" layoutY="10.0" prefWidth="10.0" visible="false" />
            <Button fx:id="moveupButton" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Move up" />
            <Separator layoutX="124.0" layoutY="10.0" prefWidth="10.0" visible="false" />
            <Button fx:id="movedownButton" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="80.0" text="Move down" />
         </children>
      </HBox>
   </children>
</VBox>
