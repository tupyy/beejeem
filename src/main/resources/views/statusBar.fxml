<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="masterPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="30.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stes.isami.bjm.gui.mainview.statusBar.StatusBarController">
   <children>
      <HBox alignment="CENTER_LEFT" minWidth="400.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Tcp Server Status: " HBox.hgrow="NEVER" />
            <Label fx:id="tcpServerStatusLabel" text="Excel client not connected" HBox.hgrow="ALWAYS" />
            <Separator maxWidth="2000.0" visible="false" HBox.hgrow="ALWAYS" />
         </children></HBox>
      <HBox maxWidth="2000.0" prefWidth="2000.0" HBox.hgrow="ALWAYS" />
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <HBox fx:id="connectHBox" alignment="CENTER" maxWidth="-Infinity" prefWidth="100.0" HBox.hgrow="NEVER">
         <children>
            <ImageView fx:id="imageViewer" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin></ImageView>
            <Label fx:id="sshLabel" minWidth="-Infinity" prefHeight="17.0" prefWidth="100.0" text="Label" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
